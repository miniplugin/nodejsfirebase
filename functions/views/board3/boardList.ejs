<%- include ("../common/header") %>
	<a href="boardForm">New</a>

	<table border="1" style="width:600px">
		<colgroup>
			<col width='8%' />
			<col width='*%' />
			<col width='15%' />
			<col width='15%' />
		</colgroup>
		<thead>
			<tr>
				<th>No.</th>
				<th>Title</th>
				<th>Writer</th>
				<th>Email</th>
				<th>Date</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0; i<rows.length; i++) { %>
				<tr>
					<td>
						<%=rows.length-i%>
					</td>
					<td><a href='boardRead?brdno=<%=rows[i].brdno%>'>
							<%=rows[i].brdtitle%>
						</a></td>
					<td>
						<%=rows[i].brdwriter%>
					</td>
					<td>
						<%=rows[i].brdEmail%>
					</td>
					<td>
						<%=rows[i].brddate%>
					</td>
				</tr>
				<% } %>
		</tbody>
	</table>
	<%- include ("../common/footer") %>